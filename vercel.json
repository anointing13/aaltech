{
  "builds": [
      {
          "src": "aaltech/wsgi.py", 
          "use": "@vercel/python", 
          "config": { 
              "maxLambdaSize": "50mb", 
              "runtime": "python3.8" 
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "aaltech/wsgi.py"
      }
  ]
}
